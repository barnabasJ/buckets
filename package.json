{
  "name": "buckets",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=production ts-node --esm ./server.ts",
    "dev": "ts-node --esm ./server.ts",
    "cloudflare-dev": "wrangler dev dist/server.js",
    "deploy": "wrangler deploy --minify dist/server.js",
    "build": "npm run build:server && npm run build:client",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr ./src/server.tsx"
  },
  "dependencies": {
    "@fastify/middie": "^8.3.0",
    "@fastify/static": "^6.12.0",
    "@vanilla-extract/css": "^1.14.0",
    "fastify": "^4.25.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^5.0.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230914.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vanilla-extract/vite-plugin": "^3.9.3",
    "@vitejs/plugin-react": "^4.2.1",
    "wrangler": "^3.15.0"
  }
}
